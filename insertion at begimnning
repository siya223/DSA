#include<stdio.h>
int main(){
    #include<stdio.h>
#define MAX 100
int list[MAX];
int n = 0;
void insertinbeg(int value){
    if(n ==  MAX){
        printf("List is full !!!!\n");
        return;
    }
     for(int i = n-1; i>=0; i--){
         list[i+1] = list[i];
     }
     list[0] = value;
     n++;
     printf("Element Inserted !!!\n");
}

void display list(){
    if (n == 0){
        printf("list is empty !!!\n");
        return;
    }        

    for(int i=0; i<n; i++){
        printf("%d\t", list[i]);
    }
    printf("\n");
}    

